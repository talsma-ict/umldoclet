<?xml version="1.0" encoding="UTF-8"?>
<!-- Travis CI based toolchain file -->
<toolchains>
    <!-- JDK toolchains -->
    <toolchain>
        <type>jdk</type>
        <provides>
            <version>1.8</version>
            <vendor>oracle</vendor>
        </provides>
        <configuration>
            <jdkHome>/usr/lib/jvm/java-8-oracle</jdkHome>
        </configuration>
    </toolchain>
    <toolchain>
        <type>jdk</type>
        <provides>
            <version>9</version>
            <vendor>oracle</vendor>
        </provides>
        <configuration>
            <jdkHome>/usr/lib/jvm/java-9-oracle</jdkHome>
        </configuration>
    </toolchain>
</toolchains>
